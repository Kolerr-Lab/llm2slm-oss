{
  "name": "llm2slm-vscode",
  "displayName": "LLM2SLM VSCode Extension",
  "description": "VSCode extension for LLM2SLM - Convert Large Language Models to Small Language Models",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:llm2slm.convert",
    "onCommand:llm2slm.infer",
    "onCommand:llm2slm.serve"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "llm2slm.convert",
        "title": "LLM2SLM: Convert Model",
        "category": "LLM2SLM"
      },
      {
        "command": "llm2slm.infer",
        "title": "LLM2SLM: Run Inference",
        "category": "LLM2SLM"
      },
      {
        "command": "llm2slm.serve",
        "title": "LLM2SLM: Start Server with UI",
        "category": "LLM2SLM"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "llm2slm.convert",
          "when": "workspaceFolderCount != 0"
        },
        {
          "command": "llm2slm.infer",
          "when": "workspaceFolderCount != 0"
        },
        {
          "command": "llm2slm.serve",
          "when": "workspaceFolderCount != 0"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kolerr-Lab/llm2slm-oss"
  },
  "publisher": "kolerr-lab",
  "license": "MIT"
}
